 #include <stdio.h>

int main() {

    int n;

    scanf("%d", &n);

    if (n == 0) {

        printf("Zero\n");

        return 0;

    }

    int reversed_n = 0;

    int original_n = n;

    int remainder;

    

    // Count trailing zeros to be printed later

    int trailing_zeros = 0;

    if (n > 0) {

        while (n % 10 == 0 && n != 0) {

            trailing_zeros++;

            n /= 10;

        }

    }

    // Reverse the number (excluding trailing zeros)

    while (n != 0) {

        remainder = n % 10;

        reversed_n = reversed_n * 10 + remainder;

        n /= 10;

    }

    // Print words for the reversed number

    while (reversed_n != 0) {

        remainder = reversed_n % 10;

        switch (remainder) {

            case 0:

                printf("Zero");

                break;

            case 1:

                printf("One");

                break;

            case 2:

                printf("Two");

                break;

            case 3:

                printf("Three");

                break;

            case 4:

                printf("Four");

                break;

            case 5:

                printf("Five");

                break;

            case 6:

                printf("Six");

                break;

            case 7:

                printf("Seven");

                break;

            case 8:

                printf("Eight");

                break;

            case 9:

                printf("Nine");

                break;

        }

        reversed_n /= 10;

        if (reversed_n != 0 || trailing_zeros > 0) {

            printf(" ");

        }

    }

    // Print words for the original trailing zeros

    for (int i = 0; i < trailing_zeros; i++) {

        printf("Zero");

        if (i < trailing_zeros - 1) {

            printf(" ");

        }

    }

    

    printf("\n");

    return 0;

}
