  #include <stdio.h>

#include <math.h>

int main() {

    int n, originalN, remainder, digits = 0;

    double result = 0.0;

    scanf("%d", &n);

    originalN = n;

    int temp = originalN;

    while (temp != 0) {

        temp /= 10;

        digits++;

    }

    temp = originalN;

    while (temp != 0) {

        remainder = temp % 10;

        result += pow(remainder, digits);

        temp /= 10;

    }

    if ((int)result == originalN) {

        printf("Armstrong\n");

    } else {

        printf("Not Armstrong\n");

    }

    return 0;

}