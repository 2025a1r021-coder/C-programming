#include <stdio.h>

int main() {
    int N; 
    
    
    scanf("%d", &N);
    
    int array[N]; 
    long long sum = 0; 
                       
    
  
    for (int i = 0; i < N; i++) {
        scanf("%d", &array[i]);
        sum += array[i]; 
    }
    
    
    printf("%lld\n", sum);
    
    return 0;
}


