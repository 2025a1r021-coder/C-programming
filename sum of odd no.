 #include <stdio.h>

int main() {

    int N, sum = 0;

    

    // Read the input integer N

    scanf("%d", &N);

    

    // Calculate the sum of odd numbers from 1 to N

    for (int i = 1; i <= N; i++) {

        if (i % 2 != 0) {

            sum += i;

        }

    }

    

    // Print the final sum

    printf("%d", sum);

    

    return 0;

}