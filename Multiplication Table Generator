 #include <stdio.h>

int main() {

    int N;

    

    // Read the input integer N

    scanf("%d", &N);

    

    // Loop from 1 to 10 to generate the multiplication table

    for (int i = 1; i <= 10; i++) {

        // Print each line of the multiplication table in the specified format

        printf("%d x %d = %d\n", N, i, N * i);

    }

    

    return 0;

}
