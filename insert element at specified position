#include <stdio.h>

int main() {
    int n, i, x, p;

  
    scanf("%d", &n);

    int arr[100];  

  
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    
    scanf("%d", &x);

    
    scanf("%d", &p);

   
    for(i = n; i > p; i--) {
        arr[i] = arr[i - 1];
    }

    arr[p] = x;

    n++;  

    
    for(i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
